		    		showAUIError(response, AJS.$('.message-bar'));
    				AJS.$('#zephyr-je-dialog-save').attr('disabled', 'disabled');
    				AJS.$('#dialog-icon-wait').hide();
    				return;
    			} else {
    				if(errorCallback)
        				errorCallback.call();
    			}
